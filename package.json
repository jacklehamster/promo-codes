{
  "name": "@dobuki/promo-codes",
  "module": "src/index.ts",
  "description": "Hookup your server to distribute promo codes",
  "version": "1.0.7",
  "keywords": [
    "bun",
    "bunjs",
    "typescript",
    "template",
    "npm",
    "np",
    "publish",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jacklehamster/promo-codes.git"
  },
  "homepage": "<fill in homepage>",
  "author": {
    "name": "Vincent Le Quang",
    "email": "jacklehamster@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "bun test",
    "sample": "bun run src/sample.ts",
    "build": "bun i && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration && bun link",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "build:example": "cd example && bun i && bun run build && cd ..",
    "example": "cd example && bun start && cd ..",
    "fix-package-name": "npm explore package-name-fixer -- bun start \"$(pwd)\"",
    "auto-publish": "npm explore @dobuki/auto-publish -- bun run auto-publish \"$(pwd)\""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dobuki/dev-dependencies": "^1.0.20",
    "@types/connect-redis": "^0.0.23",
    "@types/express-session": "^1.18.1",
    "@types/redis": "^4.0.11",
    "@dobuki/google-sheet-db": "^1.0.8",
    "connect-redis": "^8.0.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "linkify-html": "^4.2.0",
    "linkifyjs": "^4.2.0",
    "redis": "^4.7.0"
  }
}
